#: ../plugins/ejecter/ejecter.c:172
msgid "Drive was removed without ejecting"
msgstr "Zariadenie bolo odstránené bez odpojenia"

#: ../plugins/ejecter/ejecter.c:172
msgid "Please use menu to eject before removal"
msgstr "Použite, prosím, ponuku na odpojenie pred odstránením"

#: ../plugins/ejecter/ejecter.c:200
#, c-format
msgid "%s has been ejected"
msgstr "%s bolo odpojené"

#: ../plugins/ejecter/ejecter.c:201
msgid "It is now safe to remove the device"
msgstr "Teraz môžete zariadenie bezpečne odstrániť"

#: ../plugins/ejecter/ejecter.c:206
#, c-format
msgid "Failed to eject %s"
msgstr "Nepodarilo sa odpojiť %s"

#: ../plugins/ejecter/ejecter.c:588
msgid "Select a drive in menu to eject safely"
msgstr "Vyberte zariadenie, ktoré chcete bezpečne odpojiť"

#: ../plugins/ejecter/ejecter.c:642 ../plugins/ejecter/ejecter.c:652
msgid "Ejecter"
msgstr "Bezpečné odpojenie"

#: ../plugins/ejecter/ejecter.c:644
msgid "Hide icon when no devices"
msgstr "Skryť ikonu pri chýbajúcich zariadeniach"

#: ../plugins/ejecter/ejecter.c:653
msgid "Ejects mounted drives"
msgstr "Odpojí pripojené zariadenia"
